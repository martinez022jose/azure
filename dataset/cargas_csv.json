{
	"name": "cargas_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "input_datalake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"file_name": {
				"type": "string"
			},
			"folder": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().file_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().folder",
					"type": "Expression"
				},
				"fileSystem": "files-csvs"
			},
			"columnDelimiter": ";",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "ID;ID_Name;Fecha;Nro_Maquina;Funciono;CantidadCargas;Volumen (m3);Hora fin",
				"type": "String"
			}
		]
	}
}